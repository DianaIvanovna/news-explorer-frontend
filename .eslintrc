{
    "extends": "airbnb-base",
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id","_setHandlers"] }],
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
      "no-console": "off",
      "global-require": 0,
      "func-names": ["error", "never"]
   },
   "env": {
    "browser": true
  }
}