{
    "extends": "airbnb-base",
    "rules": {
      "no-underscore-dangle": "off",
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
      "no-console": "off",
      "global-require": 0,
      "func-names": ["error", "never"],
      "class-methods-use-this":"off"
   },
   "env": {
    "browser": true
  }
}